<view class='topInfo'>
    <image src='/image/1.png'></image>
</view >

<view style="position:absolute; top:290rpx;width:100%;">
  <view class='topInfo-title'>
  <text class='fontTitle'>我是标题我是标题</text>
  </view>
  <view class='subforum-detail-item' wx:for='{{subforum}}'>
      <view class='subforum-detail-username'>
          <image src='/image/personal-center.png'></image>
          <text class='fontNickname'>{{item.username}}</text>
          <view class="subforum-detail-level">
            <text>1</text>
          </view>
          <view wx:if="{{item.ifMyself}}" class="master"><text>楼主</text></view>
      </view>
      <view class='subforum-detail-content fontItemDetail'>
          <text>{{item.content}}</text>
      </view>
      <view class='subforum-detail-date fontDate'>
          <text>{{item.date}}</text>
      </view>
  </view>
</view>
